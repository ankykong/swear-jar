{
  "name": "swear-jar",
  "version": "1.0.0",
  "description": "Virtual swear jar with multi-user access and Supabase Edge Functions",
  "scripts": {
    "dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "vercel-build": "npm run install-all && npm run build",
    "deploy": "vercel --prod",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:functions": "supabase functions serve",
    "supabase:deploy": "supabase functions deploy"
  },
  "keywords": [
    "fintech",
    "plaid",
    "money",
    "swear-jar",
    "vercel",
    "supabase",
    "edge-functions"
  ],
  "author": "SwearJar Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.5"
  },
  "devDependencies": {
    "vercel": "^44.5.1"
  }
}
